<class name="Port2Event">
	<scxml initial="main">
		<state id="main">
			<transition port="tkinter_input" event="input" target=".">
				<parameter name="character"/>
				<raise event="tkinter_input" scope="broad">
					<parameter expr="character"/>
				</raise>
				<script>
					print("RAISING EVENT" + str(character))
				</script>
			</transition>
		</state>
	</scxml>
</class>