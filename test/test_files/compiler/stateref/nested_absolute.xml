<?xml version="1.0" ?>
<diagram
  xmlns="msdl.uantwerpen.be/sccd"
  author="Joeri Exelmans"
  name="take_one">
  <description>
    Transition target reference: relative path
  </description>
  <outport name="out"/>
  <class name="c" default="true">
    <scxml initial="a">
      <state id="a">
        <state id="b">
          <state id="c">
            <transition target="/d/e/f"/>
          </state>
        </state>
      </state>
      <state id="d">
        <state id="e">
          <state id="f">
            <onentry>
              <raise event="done" port="out"/>
            </onentry>
          </state>
        </state>
      </state>
    </scxml>
  </class>
  <test>
    <expected>
      <slot>
        <event name="done" port="out"/>
      </slot>
    </expected>
  </test>
</diagram>
